
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>WebApiResult</title>
    <script src="~/scripts/jquery-1.10.2.min.js"></script>
    <script type="text/javascript">
        $(function () {
            //无返回值
            $("#btn1").click(function () {
                $.ajax({
                    type: "post",
                    data: null,
                    url: "/api/postresult/NoResult",
                    success: function (data, status) {
                        alert(data);
                    }
                });
            });
            //返回Json
            $("#btn2").click(function () {
                $.ajax({
                    type: "post",
                    data: null,
                    url: "/api/postresult/JsonResult",
                    success: function (data, status) {
                        alert(data);
                        alert(data[0].id);
                    }
                });
            });
            //返回匿名对象
            $("#btn3").click(function () {
                $.ajax({
                    type: "post",
                    data: null,
                    url: "/api/postresult/AnonymousResult",
                    success: function (data, status) {
                        alert(data);
                        alert(data[0].id);
                    }
                });
            });
            //返回OK
            $("#btn4").click(function () {
                $.ajax({
                    type: "post",
                    data: null,
                    url: "/api/postresult/OKResult",
                    success: function (data, status) {
                        alert(data);
                    }
                });
            });
            //返回OK重载方法
            $("#btn5").click(function () {
                $.ajax({
                    type: "post",
                    data: null,
                    url: "/api/postresult/OKOverLoadResult",
                    success: function (data, status) {
                        alert(data);
                    }
                });
            });
        });
    </script>
</head>
<body>
    <div>
        <input type="button" id="btn1" value="没有返回值" /><br /><br />
        <input type="button" id="btn2" value="返回Json<T>(T content)" /><br /><br />
        <input type="button" id="btn3" value="返回匿名对象" /><br /><br />
        <input type="button" id="btn4" value="返回OK" /><br /><br />
        <input type="button" id="btn5" value="返回OK重载的方法" /><br /><br />
    </div>
</body>
</html>
